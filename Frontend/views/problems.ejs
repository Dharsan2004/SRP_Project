<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta
            name="viewport"
            content="width=device-width, initial-scale=1, shrink-to-fit=no"
        />

        <link
            href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap"
            rel="stylesheet"
        />

        <title>Interview Insight Hub</title>
        <style>
            .logo img {
                max-width: 120px;
                height: 70px;
            }
            nav.main-nav {
                display: flex;

                padding: 10px 10px; /* Adjust the padding to increase the height */
            }
            .sidebar {
                position: fixed;
                top: 0;
                right: 0;
                height: 100%;
                width: 250px;
                background-color: #f4f4f4;
                padding: 20px;
                box-shadow: -5px 0 10px rgba(0, 0, 0, 0.1);
                z-index: 1000;
                overflow-y: auto;
            }

            .sidebar h3 {
                color: #333;
            }

            .sidebar ul {
                list-style: none;
                padding: 0;
                margin: 0;
            }

            .sidebar li {
                margin-bottom: 10px;
            }

            .sidebar a {
                text-decoration: none;
                color: #08769e;
            }

            .sidebar a:hover {
                color: #5ea4da;
            }

            .main-content {
                margin-right: 250px; /* Adjust margin to accommodate the sidebar width */
            }
        </style>
        <!-- Bootstrap core CSS -->
        <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

        <!-- Additional CSS Files -->
        <link rel="stylesheet" href="/css/fontawesome.css" />
        <link rel="stylesheet" href="/css/templatemo-woox-travel.css" />
        <link rel="stylesheet" href="/css/owl.css" />
        <link rel="stylesheet" href="/css/animate.css" />
        <link
            rel="stylesheet"
            href="https://unpkg.com/swiper@7/swiper-bundle.min.css"
        />
        <!--

TemplateMo 580 Woox Travel

https://templatemo.com/tm-580-woox-travel

-->
    </head>

    <body>
        <!-- ***** Preloader Start ***** -->
        <div id="js-preloader" class="js-preloader">
            <div class="preloader-inner">
                <span class="dot"></span>
                <div class="dots">
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
            </div>
        </div>
        <!-- ***** Preloader End ***** -->

        <!-- ***** Header Area Start ***** -->
        <header class="header-area header-sticky">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <nav class="main-nav">
                            <!-- ***** Logo Start ***** -->
                            <!--a href="index.html" class="logo">
                        <img src="assets/images/job_interview.jpg" alt="">
                    </a-->
                            <!-- ***** Logo End ***** -->
                            <!-- ***** Menu Start ***** -->
                            <ul class="nav">
                                <li><a href="/">Home</a></li>
                                <li><a href="/about">About</a></li>
                                <li>
                                    <a href="/newexperience">New Experience</a>
                                </li>
                                <li>
                                    <a href="/problems" class="active"
                                        >Problems</a
                                    >
                                </li>
                                <li><a href="#">Logout</a></li>
                            </ul>
                            <a class="menu-trigger">
                                <span>Menu</span>
                            </a>
                            <!-- ***** Menu End ***** -->
                        </nav>
                    </div>
                </div>
            </div>
        </header>
        <!-- ***** Header Area End ***** -->
        <!--Main Bannner Area End ***** -->

        <div class="visit-country">
            <div class="container">
                <div class="row">
                    <div class="col-lg-5">
                        <div class="section-heading">
                            <h2>PROBLEMS</h2>
                        </div>
                    </div>
                </div>
                <div class="sidebar">
                    <h3>Filter <i class="fas fa-filter"></i></h3>
                    <br />
                    <ul class="category-list">
                        <li>
                            Company <i class="fas fa-tv"></i>
                            <ul>
                                <li><a href="#">Arcesium</a></li>
                                <li><a href="#">Amazon</a></li>
                                <li><a href="#">Facebook</a></li>
                                <li><a href="#">Wells Fargo</a></li>
                                <li><a href="#">SAP Labs</a></li>
                                <!-- Add more company options here -->
                            </ul>
                        </li>
                        <br />
                        <li>
                            Topics <i class="fas fa-book"></i>
                            <ul>
                                <li>
                                    <a href="#"
                                        >Data Structures and Algorithms</a
                                    >
                                </li>
                                <li><a href="#">Computer Networks</a></li>
                                <li><a href="#">DBMS</a></li>
                                <li><a href="#">Operating Systems</a></li>
                                <!-- Add more department options here -->
                            </ul>
                        </li>
                        <br />
                        <li>
                            Type <i class="fas fa-graduation-cap"></i>
                            <ul>
                                <li><a href="#">Placement</a></li>
                                <li><a href="#">Internship</a></li>
                                <!-- Add more category type options here -->
                            </ul>
                        </li>
                    </ul>
                    <br />
                    <!-- Sorting options -->
                    <div class="sorting-section">
                        <h3>Sort</h3>
                        <div class="sort-option">
                            <i class="fas fa-sort-amount-up"></i>
                            <a href="#">Easy to Difficult</a>
                        </div>
                        <div class="sort-option">
                            <i class="fas fa-sort-amount-down"></i>
                            <a href="#">High to Low Upvotes</a>
                        </div>

                        <!-- Add more sorting options with corresponding Font Awesome icons -->
                    </div>
                </div>
                <div class="row">
                    <% Interviews.forEach(interview => { %>

                    <div class="col-lg-8">
                        <div class="items">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="item">
                                        <div class="row">
                                            <div class="col-lg-8 col-sm-7">
                                                <div class="right-content">
                                                    <a
                                                        href="<%= interview.problemLink %>"
                                                        ><%=
                                                        interview.problemLink
                                                        %></a
                                                    >

                                                    <ul class="info">
                                                        <li>
                                                            <button
                                                                class="thumbs-up-btn"
                                                                onclick="upvoteProblem('<%= interview._id %>')"
                                                            >
                                                                <i
                                                                    class="fa fa-thumbs-up"
                                                                ></i>
                                                            </button>
                                                            <%=
                                                            interview.upvoted %>
                                                            Upvotes
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <% }); %>
                </div>
            </div>
        </div>
        <div><br /></div>

        <!-- Scripts -->
        <!-- Bootstrap core JavaScript -->
        <script src="vendor/jquery/jquery.min.js"></script>
        <script src="vendor/bootstrap/js/bootstrap.min.js"></script>
        <script src="/js/frontendScript.js"></script>
        <script src="/js/isotope.min.js"></script>
        <script src="/js/owl-carousel.js"></script>
        <script src="/js/wow.js"></script>
        <script src="/js/tabs.js"></script>
        <script src="/js/popup.js"></script>
        <script src="/js/custom.js"></script>

        <script>
            function bannerSwitcher() {
                next = $(".sec-1-input")
                    .filter(":checked")
                    .next(".sec-1-input");
                if (next.length) next.prop("checked", true);
                else $(".sec-1-input").first().prop("checked", true);
            }

            var bannerTimer = setInterval(bannerSwitcher, 5000);

            $("nav .controls label").click(function () {
                clearInterval(bannerTimer);
                bannerTimer = setInterval(bannerSwitcher, 5000);
            });
        </script>
    </body>
</html>
